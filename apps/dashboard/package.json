{
  "name": "dashboard",
  "version": "0.1.21",
  "private": true,
  "scripts": {
    "_dev": "NODE_OPTIONS=\"--max_old_space_size=8192\" npm run dotenv -- next dev | pino-pretty --colorize",
    "dev": "concurrently \"pnpm _dev\" \"pnpm worker:datasource-loader\"",
    "dev:all:test": "NODE_ENV=test APP_ENV=test concurrently \"pnpm dev -p 3042\" \"pnpm worker:datasource-loader\"",
    "build": "npm run dotenv -- next build",
    "tunnel": "npm run dotenv -- pnpx localtunnel -- --port 3000 --subdomain $USER-chaindesk",
    "start": "npm run dotenv -- next start",
    "typecheck": "npm run dotenv -- tsc --noemit",
    "lint": "npm run dotenv -- next lint --fix",
    "e2e:api": "jest __tests__/api",
    "e2e:api:watch": "jest __tests__/api --watch",
    "embeds:dev": "rm -rf tests/embeds/dist && mkdir -p tests/embeds/dist && concurrently \"node esbuild.config.mjs\"",
    "embeds:build": "rm -rf tests/embeds/dist && mkdir -p tests/embeds/dist && concurrently \"NODE_ENV=production node esbuild.config.mjs\"",
    "embeds:publish": "pnpm embeds:build && rm -rf ../../packages/embeds/dist && cp -r tests/embeds/dist ../../packages/embeds && cd ../../packages/embeds && npm version patch && npm publish --access public",
    "embeds:legacy:publish": "pnpm embeds:build && rm -rf ../../packages/chat-bubble/dist && cp -r tests/embeds/dist ../../packages/chat-bubble && cd ../../packages/chat-bubble && npm version patch && npm publish --access public",
    "build:widget:prestashop": "cd widgets/prestashop && bash build.sh",
    "build:widget:wordpress": "cd widgets/wordpress && bash build.sh",
    "worker:datasource-loader": "npm run dotenv -- tsx -- --watch workers/datasource-loader.ts | pino-pretty --colorize",
    "dotenv": "dotenv -e ../../.env.local",
    "set-env:prod": "cat .env.prod.local | xargs flyctl --config fly/fly.toml secrets set",
    "stripe:dev": "stripe listen --forward-to localhost:3000/api/stripe/webhook",
    "sendDailyLeads": "npm run dotenv -- tsx cron/daily-leads.tsx",
    "sendWeeklyAdminStats": "npm run dotenv -- tsx cron/admin-weekly-report.tsx",
    "sendAccountCleaningWarning": "npm run dotenv -- tsx cron/account-cleaning-warning.tsx",
    "syncDatasources": "npm run dotenv -- tsx cron/sync-datasources.tsx",
    "test:emails:trigger:receive": "npm run dotenv -- tsx tests/email-inbox/receive-email.ts"
  },
  "alias": {
    "@app/components": "./components",
    "@app/hooks": "./hooks",
    "buffer": false,
    "process": false
  },
